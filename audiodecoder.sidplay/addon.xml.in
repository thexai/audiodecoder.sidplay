<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="audiodecoder.sidplay"
  version="22.0.0"
  name="Sidplay Audio Decoder"
  provider-name="spiff">
  <requires>@ADDON_DEPENDS@</requires>
  <extension
    point="kodi.audiodecoder"
    name="sid"
    tags="true"
    tracks="true"
    library_@PLATFORM@="@LIBRARY_FILENAME@">
    <support>
      <extension name=".sid">
        <description>30100</description>
        <icon>icon.png</icon>
      </extension>
    </support>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>@PLATFORM@</platform>
    <license>GPL-2.0-or-later</license>
    <source>https://github.com/xbmc/audiodecoder.sidplay</source>
    <assets>
      <icon>icon.png</icon>
    </assets>
    <summary lang="ca_ES">Descodificador d&apos;àudio SID (MOS 6581)</summary>
    <summary lang="da_DK">SID (MOS 6581) lydafkoder</summary>
    <summary lang="de_DE">SID (MOS 6581) Audio Decoder</summary>
    <summary lang="en_GB">SID (MOS 6581) Audio Decoder</summary>
    <summary lang="es_ES">Decodificador de Audio SID (MOS 6581)</summary>
    <summary lang="fr_FR">Décodeur audio SID (MOS 6581)</summary>
    <summary lang="hr_HR">SID (MOS 6581) zvučni dekôder</summary>
    <summary lang="it_IT">SID (MOS 6581) Audio Decoder</summary>
    <summary lang="ko_KR">SID (MOS 6581) 오디오 디코더</summary>
    <summary lang="ru_RU">Аудиодекодер SID (MOS 6581)</summary>
